{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "test:unit": "tsdx test test/unit/",
    "test:integration": "tsdx test test/integration/",
    "lint": "tsdx lint",
    "prepare": "cd ../ && husky install ./sdk/.husky && cd sdk/ && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "commit": "cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "name": "tilia-nodejs-sdk",
  "author": "BMacLinden",
  "module": "dist/tilia-nodejs-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/tilia-nodejs-sdk.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/tilia-nodejs-sdk.esm.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt": "^16.0.0",
    "@size-limit/preset-small-lib": "^7.0.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^14.1.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./sdk/node_modules/cz-conventional-changelog"
    }
  }
}
